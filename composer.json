{
    "name": "themachinarium/macher-excel",
    "description": "macher excel export hook",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "macher",
            "email": "772364230@qq.com"
        }
    ],
    "require": {
        "maatwebsite/excel":"^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "tcg/voyager": "^1.4",
        "orchestra/testbench-browser-kit": ">=4.0",
        "laravel/browser-kit-testing": ">=5.0"
    },
    "autoload": {
        "psr-4": {
            "themachinarium\\MacherExcel\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "themachinarium\\MacherExcel\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "themachinarium\\MacherExcel\\VoyagerExcelServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit",
        "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s"
    }
}
